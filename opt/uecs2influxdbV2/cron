#/bin/bash -c 'source /home/pi/myenv/bin/activate && exec python /opt/uecs2influxdbV2/abc_aggregate.py'
#/bin/bash -c 'source /home/pi/myenv/bin/activate && exec python /opt/uecs2influxdbV2/replicate.py'

# Run abc_aggregate.py every day at midnight
# 集計は1日毎に実施
0 0 * * * /bin/bash -c 'source /home/pi/myenv/bin/activate && exec python /opt/uecs2influxdbV2/abc_aggregate.py'

# Run replicate.py every 10 minutes
# クラウド等へのデータ連携は10分ごとに実施
*/10 * * * * /bin/bash -c 'source /home/pi/myenv/bin/activate && exec python /opt/uecs2influxdbV2/replicate.py'
